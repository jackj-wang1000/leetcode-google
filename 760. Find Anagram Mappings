class Solution {
    public int[] anagramMappings(int[] A, int[] B) {
        int L = A.length;
        int[] p = new int[L];
        Map<Integer,Stack<Integer>> map = new HashMap<Integer,Stack<Integer>>();
        for (int i = 0; i < L; i++) {
            if (!map.containsKey(B[i])) map.put(B[i],new Stack<Integer>());
            Stack<Integer> s = map.get(B[i]);
            s.push(i);
        }
        for (int i = 0; i < L; i++) {
            p[i] = map.get(A[i]).pop();
        }
        return p;
    }
}
